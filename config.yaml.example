albums:
  - id: "album-uuid-from-immich"
    slug: "family-vacation"
    title: "Family Vacation"

scan:
  intervalMinutes: 60


gallery:
  engine: "thumbsup"
  flags:
    - "--theme"
    - "cards"
    - "--title"
    - "Gallery"
    - "--cleanup"
    - "true"
    - "--sort-media-direction"
    - "desc"

deploy:
  method: cloudflare
  cloudflare:
    projectName: "static-immich"
